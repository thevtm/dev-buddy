
<div class="container z-depth-3 center-align white" style="border-radius: 10px; margin-bottom: 30px;">
  <%= render "shared/navbars/profile" %>

  <div class="user-partial-profil">
    <div class="col s12">
      <div class="collection-item avatar" style="padding-top: 30pt;">
        <img class="avatar-user circle responsive-img z-depth-2" style="
        width: 150px; height: 150px;
        margin: 1px;
        border: 2px solid grey;
        border-color: #64ffda;
        border-radius: 500px;
        -webkit-border-radius: 500px;
        -moz-border-radius: 500px;" src= '<%= image_path "adultw.jpg" %>'>
