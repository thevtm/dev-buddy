        </div>

        <div>
          <h4>
            <span class="user-name-font-header"><%= user.name %></span>
          </h4>
        </div>

        <div>
          <h4>
            <span class="user-show__name title-user white-text"><%= user.job_position %></span>
          </h4>
        </div>
        <span class="user-show__location white-text"><%= user.location %></span>

    <div class="divider divider-partial teal accent-2 center-align"></div>

    <!-- end of picture and name -->
    <div class="container container-index">


      <div class="user-goals-skills  z-depth-2">

